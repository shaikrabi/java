class number {
    static int arr[] = { 20,45,9,80,77};
    static int largest() {
    
        int max =arr[0];
        
         for (int i = 1; i < arr.length; i++)
        if (arr[i] > max)
        
        max = arr[i];
        
        return max;
    }
    public static void main(String [] args) {
        System.out.println(largest());
    }
}
