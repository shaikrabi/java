public class NestedLoopExample {
    public static void main(String[] args) {

        for (int i = 1; i <= 3; i++) {
            System.out.println("i = " + i);  // Print current value of i

            for (int j = 1; j <= 2; j++) {
                System.out.println("   j = " + j); 
            }
            System.out.println();
        }
    }
}
